<odoo>
    <data>
        <record id="view_sale_order_form" model="ir.ui.view">
            <field name="name">Sales Order Form</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="groups_id" eval="[(4,ref('gate_app.group_sale_salesman_viewer'))]"/>
            <field name="arch" type="xml">
                <xpath expr="//form" position="attributes">
                    <attribute name="edit">false</attribute>
                    <attribute name="create">false</attribute>
                </xpath>
            </field>
        </record>
        <menuitem id="sale.report_sales_team"
            name="Sales Teams"
            parent="sale.sale_menu_root"
            groups="group_sale_salesman_viewer"
            action="sales_team.crm_team_action_sales"
            sequence="3"/>

         <menuitem id="sale.sale_order_menu"
            name="Orders"
            groups="group_sale_salesman_viewer"
            parent="sale.sale_menu_root"
            sequence="2"/>
        <menuitem id="sale.menu_sale_order"
            name="Orders"
            action="sale.action_orders"
            parent="sale.sale_order_menu"
            sequence="2" groups="group_sale_salesman_viewer"/>
